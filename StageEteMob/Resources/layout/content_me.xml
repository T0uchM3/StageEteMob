<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	app:layout_behavior="@string/appbar_scrolling_view_behavior"
	tools:showIn="@layout/activity_main"
	android:id="@+id/relativeLayInc"
	android:minWidth="25px"
	android:minHeight="25px"
	tools:gridSpec="1|12|#0093eeff|K:#ee8700ff:16,l:72,l:16,r|S:#83ee00ff:16,0,l:16,56,l:16,0,r">
	<LinearLayout
		android:orientation="vertical"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/linearLayout14"
		android:paddingLeft="20px"
		android:paddingRight="20px">
		<TextView
			android:text="X"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingTop="30px"
			android:id="@+id/letterTV"
			android:textColor="#ff222222"
			android:textStyle="bold"
			android:textSize="100px"
			android:textAlignment="center" />



		<TextView
			android:text="name"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="#ff222222"
			android:id="@+id/nameTV"
			android:layout_marginTop="50px" />

		<com.google.android.material.divider.MaterialDivider
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/materialDivider1"
			android:layout_marginVertical="40px" />
		<TextView
			android:text="id"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="#ff222222"
			android:id="@+id/idTV" />

		<com.google.android.material.divider.MaterialDivider
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/materialDivider1"
			android:layout_marginVertical="40px" />
		<TextView
			android:text="iduser"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:textColor="#ff222222"
			android:id="@+id/iduserTV" />
		<Button
			android:text="Sign Out"
			android:layout_width="match_parent"
			android:layout_height="59.0dp"
			android:id="@+id/signoutBtn"
			android:layout_alignParentBottom="true"
			android:layout_marginTop="100px"
			android:textAllCaps="false" />

	</LinearLayout>
</RelativeLayout>
