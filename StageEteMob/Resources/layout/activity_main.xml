<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<com.google.android.material.appbar.AppBarLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:theme="@style/AppTheme.AppBarOverlay">
		<!--<androidx.appcompat.widget.Toolbar
			android:id="@+id/toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="?attr/colorPrimary"
			app:popupTheme="@style/AppTheme.PopupOverlay" />-->


	</com.google.android.material.appbar.AppBarLayout>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:id="@+id/relativeLay">


		<RelativeLayout
			android:id="@+id/containerView"
			android:paddingBottom="160px"
			android:layout_width="match_parent"
			android:layout_height="match_parent">

		</RelativeLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="4dp"
			android:id="@+id/shadow"
			android:layout_above="@id/fakeTabLayout"
			android:background="@drawable/shadow"/>
		<LinearLayout
			android:id="@+id/fakeTabLayout"
			android:orientation="horizontal"
			android:layout_alignParentBottom="true"
			android:layout_height="55.0dp"
			android:layout_width="match_parent">

			<LinearLayout
				android:layout_weight="1"
				android:layout_height="match_parent"
				android:layout_width="0dp"
				android:minWidth="25px"
				android:minHeight="25px">
				<LinearLayout
					android:orientation="vertical"
					android:minWidth="25px"
					android:minHeight="25px"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/linearLayout15">
					<ImageView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="2"
						android:src="@drawable/ns"
						android:id="@+id/imageView151"
						android:contentDescription="description"
						android:minWidth="25px"
						android:minHeight="25px"/>
					<TextView
						android:text="Client"
						android:textColor="#ff222222"
						app:autoSizeTextType="uniform"
						app:autoSizeMaxTextSize="15sp"
						app:autoSizeMinTextSize="6sp"
						app:autoSizeStepGranularity="1sp"
						android:gravity="center"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1"
						android:paddingBottom="3dp"
						android:id="@+id/textView152"/>
				</LinearLayout>

			</LinearLayout>

			<LinearLayout
				android:layout_weight="1"
				android:layout_height="fill_parent"
				android:layout_width="0dp"
				android:minWidth="25px"
				android:minHeight="25px">
				<LinearLayout
					android:orientation="vertical"
					android:minWidth="25px"
					android:minHeight="25px"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/linearLayout16">
					<ImageView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="2"
						android:src="@drawable/midb"
						android:id="@+id/imageView161"
						android:contentDescription="description"
						android:minWidth="25px"
						android:minHeight="25px"
						android:layout_marginBottom="5px"
						android:layout_marginTop="5px" />

				</LinearLayout>

			</LinearLayout>

			<LinearLayout
				android:layout_weight="1"
				android:layout_height="fill_parent"
				android:layout_width="0dp"
				android:minWidth="25px"
				android:minHeight="25px">
				<LinearLayout
					android:orientation="vertical"
					android:minWidth="25px"
					android:minHeight="25px"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:id="@+id/linearLayout17">
					<ImageView
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="2"
						android:src="@drawable/notselecteduser"
						android:id="@+id/imageView171"
						android:contentDescription="description"
						android:minWidth="25px"
						android:minHeight="25px"/>
					<TextView
						android:text="Me"
						android:textColor="#ff222222"
						app:autoSizeTextType="uniform"
						app:autoSizeMaxTextSize="15sp"
						app:autoSizeMinTextSize="6sp"
						app:autoSizeStepGranularity="1sp"
						android:gravity="center"
						android:textAppearance="?android:attr/textAppearanceMedium"
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1"
						android:id="@+id/textView172"
						android:paddingBottom="3dp"/>
				</LinearLayout>

			</LinearLayout>

		</LinearLayout>



	</RelativeLayout>

	<!--<include
		layout="@layout/content_main" />-->

</androidx.coordinatorlayout.widget.CoordinatorLayout>
